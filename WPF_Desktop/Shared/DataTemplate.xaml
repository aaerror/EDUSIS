                    xmlns:view="clr-namespace:WPF_Desktop.Views"
                    xmlns:viewModel="clr-namespace:WPF_Desktop.ViewModels">

    <DataTemplate DataType="{ x:Type viewModel:GestionAlumnosViewModel }">
        <view:GestionAlumnosView/>
    </DataTemplate>

    <DataTemplate DataType="{ x:Type viewModel:PerfilAlumnoViewModel }">
        <view:PerfilAlumnoView/>
    </DataTemplate>

    <DataTemplate DataType="{ x:Type viewModel:RegistrarAlumnoViewModel }">
        <view:RegistrarPersonaView/>
    </DataTemplate>

    <DataTemplate DataType="{ x:Type viewModel:EditarAlumnoViewModel }">
        <view:EditarAlumnoView/>
    </DataTemplate>

    <DataTemplate DataType="{ x:Type viewModel:LoginViewModel }">
        <view:LoginView/>
    </DataTemplate>
    
    
</ResourceDictionary>